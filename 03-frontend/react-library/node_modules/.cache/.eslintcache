mponents/ReturnBook.tsx",["82"],[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/HomePage/HomePage.tsx",[],[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/HomePage/components/ExploreTopBooks.tsx",["83"],[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/HomePage/components/Heros.tsx",["84","85"],[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/HomePage/components/LibraryServices.tsx",["86"],[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/SearchBooksPage/SearchBooksPage.tsx",["87","88","89","90","91","92","93"],[],"import { useEffect, useState } from \"react\";\nimport BookModel from \"../../models/BookModel\";\nimport { Pagination } from \"../Utils/Pagination\";\nimport { SpinnerLoading } from \"../Utils/SpinnerLoading\";\nimport { SearchBook } from \"./components/SearchBook\";\n\nexport const SearchBooksPage = () => {\n  const [books, setBooks] = useState<BookModel[]>([]);\n  const [isLoading, setIsLoading] = useState(true);\n  const [httpError, setHttpError] = useState(null);\n\n  // added for pagination\n  const [currentPage, setCurrentPage] = useState(1);\n  const [booksPerPage] = useState(5);\n  const [totalAmountOfBooks, setTotalAmountOfBooks] = useState(0);\n  const [totalPages, setTotalPages] = useState(0);\n\n  // added for search title\n  const [search, setSearch] = useState(\"\");\n  const [searchUrl, setSearchUrl] = useState(\"\");\n\n  // added for search category\n  const [categorySelection, setCategorySelection] = useState(\"Book category\");\n\n  // paste the useEffect from Carousel.tsx\n  useEffect(() => {\n    const fetchBooks = async () => {\n      const baseUrl: string = \"http://localhost:8080/api/books\";\n\n      // edited for pagination\n      let url: string = \"\";\n\n      // added for search\n      if (searchUrl === \"\") {\n        url = `${baseUrl}?page=${currentPage - 1}&size=${booksPerPage}`;\n      } else {\n        url = baseUrl + searchUrl;\n      }\n\n      const response = await fetch(url); // get url data\n\n      if (!response.ok) {\n        throw new Error(\"Something went wrong!\");\n      }\n\n      const responseJson = await response.json(); // convert url data into json\n      const responseData = responseJson._embedded.books; // get the books\n\n      // added for pagination\n      setTotalAmountOfBooks(responseJson.page.totalElements);\n      setTotalPages(responseJson.page.totalPages);\n\n      const loadedBooks: BookModel[] = [];\n      for (const key in responseData) {\n        loadedBooks.push({\n          id: responseData[key].id,\n          title: responseData[key].title,\n          author: responseData[key].author,\n          description: responseData[key].description,\n          copies: responseData[key].copies,\n          copiesAvailable: responseData[key].copiesAvailable,\n          category: responseData[key].category,\n          img: responseData[key].img,\n        });\n      }\n\n      setBooks(loadedBooks);\n      setIsLoading(false);\n    };\n    fetchBooks().catch((error: any) => {\n      setIsLoading(false);\n      setHttpError(error.message);\n    });\n    window.scrollTo(0, 0); // when change page, scroll to the top of window\n  }, [currentPage, searchUrl]); // whenever things in [] change, trigger useEffect\n\n  if (isLoading) {\n    return <SpinnerLoading />;\n  }\n\n  if (httpError) {\n    return (\n      <div className=\"container m-5\">\n        <p>{httpError}</p>\n      </div>\n    );\n  }\n\n  // added for search title\n  const searchHandleChange = () => {\n    if (search === \"\") {\n      setSearchUrl(\"\");\n    } else {\n      setSearchUrl(\n        `/search/findByTitleContaining?title=${search}&page=0&size=${booksPerPage}`\n      );\n    }\n  };\n\n  // add for search category\n  const categoryField = (value: string) => {\n    if (\n      value.toLowerCase() === \"fe\" ||\n      value.toLowerCase() === \"be\" ||\n      value.toLowerCase() === \"data\" ||\n      value.toLowerCase() === \"devops\"\n    ) {\n      setCategorySelection(value);\n      setSearchUrl(\n        `/search/findByCategory?category=${value}&page=0&size=${booksPerPage}`\n      );\n    } else {\n      setCategorySelection(\"All\");\n      setSearchUrl(`?page=0&size=${booksPerPage}`);\n    }\n  };\n\n  // added for pagination\n  const indexOfLastBook: number = currentPage * booksPerPage;\n  const indexOfFirstBook: number = indexOfLastBook - booksPerPage;\n  let lastItem =\n    booksPerPage * currentPage <= totalAmountOfBooks\n      ? booksPerPage * currentPage\n      : totalAmountOfBooks;\n\n  const paginate = (pageNumber: number) => setCurrentPage(pageNumber);\n\n  return (\n    <div>\n      <div className=\"container\">\n        <div>\n          <div className=\"row mt-5\">\n            <div className=\"col-6\">\n              <div className=\"d-flex\">\n                <input\n                  className=\"form-control me-2\"\n                  type=\"search\"\n                  placeholder=\"Search\"\n                  aria-labelledby=\"Search\"\n                  onChange={(e) => setSearch(e.target.value)}\n                />\n                <button\n                  className=\"btn btn-outline-success\"\n                  onClick={() => searchHandleChange()}\n                >\n                  Search\n                </button>\n              </div>\n            </div>\n            <div className=\"col-4\">\n              <div className=\"dropdown\">\n                <button\n                  className=\"btn btn-secondary dropdown-toggle\"\n                  type=\"button\"\n                  id=\"dropdownMenuButton1\"\n                  data-bs-toggle=\"dropdown\"\n                  aria-expanded=\"false\"\n                >\n                  {categorySelection}\n                </button>\n                <ul\n                  className=\"dropdown-menu\"\n                  aria-labelledby=\"dropdownMenuButton1\"\n                >\n                  <li onClick={() => categoryField(\"All\")}>\n                    <a className=\"dropdown-item\" href=\"#\">\n                      All\n                    </a>\n                  </li>\n                  <li onClick={() => categoryField(\"FE\")}>\n                    <a className=\"dropdown-item\" href=\"#\">\n                      Front End\n                    </a>\n                  </li>\n                  <li onClick={() => categoryField(\"BE\")}>\n                    <a className=\"dropdown-item\" href=\"#\">\n                      Back End\n                    </a>\n                  </li>\n                  <li onClick={() => categoryField(\"Data\")}>\n                    <a className=\"dropdown-item\" href=\"#\">\n                      Data\n                    </a>\n                  </li>\n                  <li onClick={() => categoryField(\"devops\")}>\n                    <a className=\"dropdown-item\" href=\"#\">\n                      DevOps\n                    </a>\n                  </li>\n                </ul>\n              </div>\n            </div>\n          </div>\n          {totalAmountOfBooks > 0 ? (\n            <>\n              <div className=\"mt-3\">\n                <h5>Number of results:({totalAmountOfBooks})</h5>\n              </div>\n              <p>\n                {indexOfFirstBook + 1} to {lastItem} of {totalAmountOfBooks}{\" \"}\n                items:\n              </p>\n              {books.map((book) => (\n                <SearchBook book={book} key={book.id} />\n              ))}\n            </>\n          ) : (\n            <div className=\"m-5\">\n              <h3>Can't find what you are looking for?</h3>\n              <a\n                href=\"#\"\n                type=\"button\"\n                className=\"btn main-color btn-md px-4 me-md-2 fw-bold text-white\"\n              >\n                Library Services\n              </a>\n            </div>\n          )}\n          {/* added for pagination */}\n          {totalPages > 1 && (\n            <Pagination\n              currentPage={currentPage}\n              totalPages={totalPages}\n              paginate={paginate}\n            />\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n",[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/Utils/Pagination.tsx",[],[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/Utils/SpinnerLoading.tsx",[],[],"/Users/yigezhang/Documents/CS/Web/SpringBoot/library-app/03-frontend/react-library/src/layouts/SearchBooksPage/components/SearchBook.tsx",["94"],[],"import BookModel from \"../../../models/BookModel\";\n\nexport const SearchBook: React.FC<{ book: BookModel }> = (props) => {\n  return (\n    <div className=\"card mt-3 shadow p-3 mb-3 bg-body rounded\">\n      <div className=\"row g-0\">\n        <div className=\"col-md-2\">\n          <div className=\"d-none d-lg-block\">\n            {props.book.img ? (\n              <img src={props.book.img} width=\"123\" height=\"196\" alt=\"Book\" />\n            ) : (\n              <img\n                src={require(\"../../../Images/BooksImages/book-luv2code-1000.png\")}\n                width=\"123\"\n                height=\"196\"\n                alt=\"Book\"\n              />\n            )}\n          </div>\n          <div className=\"d-lg-none d-flex justify-content-center align-items-center\">\n            {props.book.img ? (\n              <img src={props.book.img} width=\"123\" height=\"196\" alt=\"Book\" />\n            ) : (\n              <img\n                src={require(\"../../../Images/BooksImages/book-luv2code-1000.png\")}\n                width=\"123\"\n                height=\"196\"\n                alt=\"Book\"\n              />\n            )}